{
  "name": "eslint-config-everywhere",
  "version": "4.2.1",
  "description": "ESLint config for everywhere",
  "type": "module",
  "files": [
    "configs/"
  ],
  "exports": {
    ".": {
      "import": "./configs/flat/index.mjs",
      "require": "./configs/legacy/index.cjs"
    },
    "./flat": "./configs/flat/index.mjs",
    "./flat/*": "./configs/flat/*.mjs",
    "./legacy": "./configs/legacy/index.cjs",
    "./legacy/*": "./configs/legacy/*.cjs"
  },
  "scripts": {
    "lint": "eslint tests/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/locol23/eslint-config-everywhere.git"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "prettier",
    "typescript",
    "react",
    "next",
    "vitest",
    "testing-library"
  ],
  "author": "Yoshitaka Terazawa <locol6622@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/locol23/eslint-config-everywhere/issues"
  },
  "homepage": "https://github.com/locol23/eslint-config-everywhere#readme",
  "dependencies": {
    "@eslint/js": "9.38.0",
    "@next/eslint-plugin-next": "15.5.6",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-n": "17.23.1",
    "eslint-plugin-perfectionist": "4.15.1",
    "eslint-plugin-playwright": "2.2.2",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-storybook": "9.1.16",
    "eslint-plugin-testing-library": "7.13.3",
    "eslint-plugin-turbo": "2.5.8",
    "eslint-plugin-unicorn": "60.0.0",
    "@vitest/eslint-plugin": "1.0.1",
    "globals": "16.4.0",
    "typescript-eslint": "8.46.2"
  },
  "devDependencies": {
    "@playwright/test": "1.56.1",
    "@storybook/react": "9.1.16",
    "@storybook/test": "8.6.14",
    "@types/react": "19.2.2",
    "eslint": "9.38.0",
    "react": "19.2.0",
    "storybook": "9.1.16",
    "typescript": "5.9.3",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "eslint": ">=9.38.0"
  },
  "engines": {
    "node": ">= 22.21.1"
  },
  "packageManager": "pnpm@10.20.0"
}
