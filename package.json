{
  "name": "eslint-config-everywhere",
  "version": "4.1.1",
  "description": "ESLint config for everywhere",
  "type": "module",
  "files": [
    "configs/"
  ],
  "exports": {
    ".": {
      "import": "./configs/flat/index.mjs",
      "require": "./configs/legacy/index.cjs"
    },
    "./flat": "./configs/flat/index.mjs",
    "./flat/*": "./configs/flat/*.mjs",
    "./legacy": "./configs/legacy/index.cjs",
    "./legacy/*": "./configs/legacy/*.cjs"
  },
  "scripts": {
    "lint": "eslint tests/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/locol23/eslint-config-everywhere.git"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "prettier",
    "typescript",
    "react",
    "next",
    "vitest",
    "testing-library"
  ],
  "author": "Yoshitaka Terazawa <locol6622@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/locol23/eslint-config-everywhere/issues"
  },
  "homepage": "https://github.com/locol23/eslint-config-everywhere#readme",
  "dependencies": {
    "@eslint/js": "9.32.0",
    "@next/eslint-plugin-next": "15.4.7",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-n": "17.21.3",
    "eslint-plugin-perfectionist": "4.15.0",
    "eslint-plugin-playwright": "2.2.2",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-storybook": "9.0.18",
    "eslint-plugin-testing-library": "7.6.6",
    "eslint-plugin-turbo": "2.5.6",
    "eslint-plugin-unicorn": "60.0.0",
    "eslint-plugin-vitest": "0.5.4",
    "globals": "16.3.0",
    "typescript-eslint": "8.38.0"
  },
  "devDependencies": {
    "@playwright/test": "1.54.2",
    "@storybook/react": "9.0.18",
    "@storybook/test": "8.6.14",
    "@types/react": "19.1.12",
    "eslint": "9.32.0",
    "react": "19.1.1",
    "storybook": "9.0.18",
    "typescript": "5.8.3",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "eslint": ">=8.32.0"
  },
  "engines": {
    "node": ">= 22"
  },
  "packageManager": "pnpm@10.13.1"
}
